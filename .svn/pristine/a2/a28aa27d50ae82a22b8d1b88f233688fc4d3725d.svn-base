        private string DatetimeToString(DateTime? date)
        {
            if (date == null)
                return string.Empty;
            else
                return string.Format("{0:yyyy-MM-dd HH':'mm':'ss}", date);
        }

        private string DateToString(DateTime? date)
        {
            if (date == null)
                return string.Empty;
            else
                return string.Format("{0:yyyy-MM-dd}", date);
        }

