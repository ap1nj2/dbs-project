<%@ Page Title="Home Page" Language="C#" MasterPageFile="~/Site.Master" AutoEventWireup="true" CodeBehind="Default.aspx.cs" Inherits="DBS._Default" %>

<asp:Content ID="BodyContent" ContentPlaceHolderID="MainContent" runat="server">

    <div class="jumbotron">
        <p class="lead">DBSCBS Uploader File</p>
        <p>
            <asp:FileUpload ID="f_uploader" runat="server" Font-Size="Small" Height="18px" Width="665px" />
        </p>
        <p>
            <asp:Button ID="Button3" runat="server" Text="Upload" Font-Size="Small" Height="23px" OnClick="Button3_Click" Width="95px" />
        </p>
        <p>
            <asp:Label ID="Label1" runat="server" Font-Size="Smaller" Text="Name File : "></asp:Label>
            <asp:Label ID="Label2" runat="server" Font-Size="Smaller" Text="None"></asp:Label>
        </p>
        <p>
            <asp:Label ID="Label3" runat="server" Font-Size="Smaller" Text="Size File : "></asp:Label>
            <asp:Label ID="Label4" runat="server" Font-Size="Smaller" Text="None"></asp:Label>
        </p>
        <p>
            <asp:Label ID="Label5" runat="server" Font-Size="Smaller" Text="Upload Status : "></asp:Label>
            <asp:Label ID="Label6" runat="server" Font-Size="Smaller" ForeColor="Black" Text="None"></asp:Label>
        </p>
    </div>

    </asp:Content>
